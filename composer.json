{
    "name": "pocketmine/nbt",
    "description": "PHP library for working with Named Binary Tags",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "php-64bit": "*",
        "venitynetwork/binaryutils": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "1.7.7",
        "phpunit/phpunit": "^9.5",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0"
    },
    "license": "LGPL-3.0",
    "autoload": {
        "psr-4": {
            "pocketmine\\nbt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pocketmine\\nbt\\": "tests/phpunit/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
	"repositories": [
		{
			"type": "vcs",
			"url":  "git@github.com:VenityNetwork/BinaryUtils.git"
		}
	]
}
